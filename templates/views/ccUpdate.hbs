            <p>Hirabai Haridas Vidyanagri, Amrutdham, Panchavati, Nashik</p>
        </div>
    </header>
    <h1 class="blue-heading">Stud Update Data</h1>
    
    <section class="profile-section" id="studData">
    <table>
        <form action="/ccUpdate1/{{ccOneData.crn}}" method="post">
      <tbody>
        
        <tr>
            <td id="b">Profile:</td>
            <td><img src="{{getProfilePhotoUrl ccOneData.profilephoto}}" alt="svg/user.svg" width="150px" height="150px"/></td>
        </tr>
        <tr>    
            <td id="b">CRN:</td>
            <td>CRN: {{ccOneData.crn}}</td>
        </tr>
        <tr>
            <td id="b">Name:</td>
            <td><input name="name" type="text" value="{{ccOneData.name}}"/></td>
        </tr>
        <tr>
            <td id="b">Department:</td>
            <td><input name="department" type="text" value="{{ccOneData.department}}"/></td>
        </tr>
        <tr>    
            <td id="b">Class:</td>
            <td><input name="mobile" type="number" value="{{ccOneData.class}}"/></td>
        </tr>
        <tr>
            <td id="b">Mobile:</td>
            <td><input name="mobile" type="number" value="{{ccOneData.mobile}}"/></td>
        </tr>
        <tr>
            <td id="b">Email:</td>
            <td><input name="email" type="text" value="{{ccOneData.email}}"/></td>
        </tr>
        
        <tr>
        <td>
            
             <button type="submit" id="up">Update</button>
            
        </td>
    </tr>
      </tbody>
    </form>  
    </table>
        </section>
</body>
